{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case",
      "ui"
    ],
    "no-string-literal": false,
    "one-variable-per-declaration": false,
    "no-unused-expression": false,
    "no-bitwise": false,
    "no-namespace": false
  }
}
